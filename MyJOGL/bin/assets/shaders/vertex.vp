#version 330

layout(location = 0) in vec3 vertexPosition_modelspace;
layout(location = 1) in vec2 color;
layout(location = 2) in vec3 inNormal;
uniform mat4 MVP;

out vec2 uv;
out vec3 normal;

void main(){	
	gl_Position = MVP * vec4(vertexPosition_modelspace, 1.0);
	uv = color;
	normal = inNormal;
 }